{
  "name": "dushmantha-reactRedux-example",
  "version": "1.0.0",
  "description": "This is a sample web application which developed with React Js , redux and Web pack and backend service built with Node js and express JS",
  "main": "src/server/index.js",
  "scripts": {
    "client": "webpack-dev-server --config webpack/webpack.dev.js --devtool inline-source-map --hot",
    "build:client": "webpack --config webpack/webpack.prod.js",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "axios",
    "babel",
    "expressjs",
    "react router v5",
    "redux",
    "webpack"
  ],
  "author": "Dushmantha Baranige",
  "license": "ISC",
